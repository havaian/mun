# Backend API URL
VITE_BACKEND_URL=http://localhost:5000

# WebSocket URL (usually same as backend)
VITE_WEBSOCKET_URL=http://localhost:5000

# App Configuration
VITE_APP_NAME=MUN.UZ
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_OFFLINE_MODE=false

# Default Language
VITE_DEFAULT_LANGUAGE=en

# File Upload Limits
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,txt

# QR Scanner Configuration
VITE_QR_SCAN_TIMEOUT=30000
VITE_QR_RETRY_ATTEMPTS=3

# Session Configuration
VITE_SESSION_TIMEOUT_WARNING=5
VITE_SESSION_REFRESH_INTERVAL=15